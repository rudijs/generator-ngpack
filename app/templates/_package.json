{
  "name": "<%= kebabName %>",
  "version": "1.0.0",
  "description": "<%= viewName %> description",
  "main": "index.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run webpack && npm run static-assets:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "browser-sync start --config bs-config.js",
    "serve:dist": "http-server dist/",
    "static-html-ico:build": "find src -type f -regextype sed -regex '.*\\(index\\.html\\|favicon\\.ico\\).*' -exec cp -v {} build \\;",
    "static-html-ico:dist": "find src -type f -regextype sed -regex '.*\\(index\\.html\\|favicon\\.ico\\).*' -exec cp -v {} dist \\;",
    "static-images": "cp -Rv src/assets/images",
    "static-images:build": "if [ ! -d build/assets ]; then mkdir -v build/assets; fi && npm run static-images -- build/assets/images",
    "static-images:dist": "if [ ! -d dist/assets ]; then mkdir -v dist/assets; fi && npm run static-images -- dist/assets/images",
    "static-assets:build": "npm run static-html-ico:build && npm run static-images:build",
    "static-assets:dist": "npm run static-html-ico:dist && npm run static-images:dist",
    "webpack": "webpack",
    "webpack:watch": "npm run webpack -- --watch",
    "webpack:prod": "NODE_ENV=production npm run webpack",
    "lint": "eslint src",
    "lint:watch": "chokidar 'src/*.js' -c 'npm run lint'",
    "karma": "NODE_ENV=test karma start karma.conf.js",
    "karma:single": "npm run karma -- --single-run",
    "karma:ci": "npm run karma -- --single-run --browsers PhantomJS",
    "dist-clean": "rm -rfv dist/",
    "rev-bundlejs": "rev-bundlejs",
    "dist": "npm run dist-clean && npm run webpack:prod && npm run static-assets:dist && npm run rev-bundlejs"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.10.1",
    "chai": "^3.4.1",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.3.0",
    "http-server": "^0.8.5",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.1",
    "karma": "^0.13.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "ng-annotate-loader": "^0.1.0",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "rev-bundlejs": "github:rudijs/rev-bundlejs",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "yargs": "^3.31.0"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-material": "^1.0.1",
    "angular-ui-router": "^0.2.15"
  }
}
