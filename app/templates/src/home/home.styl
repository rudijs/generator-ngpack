body
  color: #212121

html
  font-size: 21px

#main_header
  position: fixed

.header_title
  font-family: Julius Sans One
  font-size: 1.3rem
  font-weight: bold

h2
  font-size: 2.5rem

h3
  font-size: 2rem

p, li
  font-family: Archivo Narrow

.highlightz
  color: #8BC34A

.wrapper
  display: flex
  flex-direction: column
  align-items: center
  // padding to allow for fixed main menu toolbar
  padding-top: 64px

.item_img
  display: flex
  justify-content: center

.clear
  clear: both

.iconz
  height: 1rem
  margin: 7px 9px 9px 9px
  fill: #000

.iconz-menu
  height: 2rem

.md-headline
  font-family: Julius Sans One

.card-content
  padding-top: 1em
